<div class="col-sm-6 col-md-3">
  <div class="thumbnail">
    <a href="/boards/<%= b.id %>" data-method='delete'>
      <div class='fa fa-times pull-right'></div>
    </a>
    <div class="caption">
      <h3><%= b.title %></h3>
      <div class="board-description"> <%= b.description %></div>
      <div class="btn-group btn-group-justified" role="group">
        <a class="btn btn-primary btn-block" href='/dashboard/<%=b.id%>'>Open</a>
      </div>
    </div>
  </div>
</div>
